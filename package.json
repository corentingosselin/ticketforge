{
  "name": "ticketforge",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --target=serve --projects=api-gateway,auth-service,event-service,ticket-service,user-service --parallel --maxParallel=4",
    "docker": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.event-service.yml -f docker/docker-compose.ticket-service.yml -f docker/docker-compose.user-service.yml -f docker/docker-compose.flyway.yml --env-file docker/.docker-compose.env up -d --force-recreate",
    "migration:generate": "npx mikro-orm migration:create --initial"
  },
  "private": true,
  "dependencies": {
    "@mikro-orm/core": "^5.6.15",
    "@mikro-orm/migrations": "^5.6.15",
    "@mikro-orm/mysql": "^5.6.15",
    "@mikro-orm/nestjs": "^5.1.8",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^9.3.12",
    "@nestjs/platform-express": "^9.0.0",
    "amqp-connection-manager": "^4.1.11",
    "amqplib": "^0.10.3",
    "argon2": "^0.30.3",
    "axios": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.6.15",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/eslint-plugin-nx": "15.9.2",
    "@nrwl/jest": "15.9.2",
    "@nrwl/js": "15.9.2",
    "@nrwl/linter": "15.9.2",
    "@nrwl/nest": "15.9.2",
    "@nrwl/node": "15.9.2",
    "@nrwl/webpack": "15.9.2",
    "@nrwl/workspace": "15.9.2",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.7.1",
    "@types/passport-local": "^1.0.35",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "15.9.2",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5"
  },
  "mikro-orm": {
    "useTsNode": true,
    "tsConfigPath": "tsconfig.orm.json",
    "configPaths": [
      "./libs/ticket-service/core/src/mikro-orm.config.ts"
    ]
  }
}
